{
        "name": "single-spa-exemple",
        "version": "1.0.0",
        "description": "",
        "main": "index.js",
        "scripts": {
                "install-all": "npm run install:config && npm run install:authors && npm run install:posts",
                "install:config": "npm i",
                "install:navbar": "cd ./packages/navbar/ && npm i",
                "install:authors": "cd ./packages/authors/ && npm i",
                "install:posts": "cd ./packages/posts/ && npm i",
                "start": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,common-deps,authors,posts\" \"npm run start:config\" \"npm run start:common-deps\" \"npm run start:authors\" \"npm run start:posts\"",
                "start:config": "webpack-dev-server --config ./webpack.config.config.dev.js --port 8233",
                "start:common-deps": "webpack-dev-server --config ./webpack.common-deps.config.dev.js --port 8234",
                "start:authors": "cd ./packages/authors/ && webpack-dev-server --config ./webpack.dev.js --port 8235",
                "start:posts": "cd ./packages/posts/ && webpack-dev-server --config ./webpack.dev.js --port 8236",
                "start:navbar": "cd ./packages/navbar/ && webpack-dev-server --config ./webpack.dev.js --port 8237",
                "build": "npm run build:config && npm run build:common-deps && npm run build:authors && npm run build:posts",
                "build:config": "webpack --config ./webpack.config.config.js -p",
                "build:common-deps": "webpack --config ./webpack.common-deps.config.js -p",
                "build:authors": "cd ./packages/authors/ && webpack --config ./webpack.config.js -p",
                "build:posts": "cd ./packages/posts/ && webpack --config ./webpack.config.js -p",
                "audit-fix": "npm run audit-fix:config && npm run audit-fix:authors && npm run audit-fix:posts",
                "audit-fix:config": "npm audit fix",
                "audit-fix:authors": "cd ./packages/authors/ && npm audit fix",
                "audit-fix:posts": "cd ./packages/posts/ && npm audit fix",
                "test": "echo \"Error: no test specified\" && exit 1",
                "postinstall": "node postinstall.js"
        },
        "author": "Carlos Henrique Escouto",
        "license": "ISC",
        "dependencies": {
                "single-spa": "^5.0.0"
        },
        "devDependencies": {
                "@babel/core": "^7.4.3",
                "@babel/plugin-syntax-dynamic-import": "7.0.0",
                "@babel/preset-env": "^7.4.3",
                "babel-core": "6.26.3",
                "babel-loader": "8.0.0",
                "clean-webpack-plugin": "0.1.19",
                "concurrently": "4.0.1",
                "copy-webpack-plugin": "4.5.2",
                "css-loader": "1.0.0",
                "style-loader": "0.23.0",
                "webpack": "^4.41.6",
                "webpack-cli": "3.1.0",
                "webpack-dev-server": "^3.1.14"
        }
}
